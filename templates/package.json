{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-phrase-record-backend",
    "stop": "egg-scripts stop --title=egg-server-phrase-record-backend",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "run": "EGG_TYPESCRIPT=true TS_NODE_FILES=1 TS_NODE_PROJECT=tsconfig.json node --require ts-node/register"
  },
  "dependencies": {
    "egg": "^2.33.1",
    "egg-onerror": "^2.1.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.15.2",
    "egg-static": "^2.2.0",
    "egg-validate": "^2.0.2",
    "lodash": "^4.17.21",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "egg-bin": "^4.16.4",
    "egg-mock": "^4.2.0",
    "eslint": "^8.5.0",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "nyc": {
    "reporter": [
      "cobertura",
      "lcov"
    ]
  },
  "author": "<%= author %>",
  "license": "MIT"
}
